<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>false</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <!-- Jangan menggunakan ProjectReference untuk proyek itu sendiri -->
  <!-- Jika Anda perlu mengakses file yang ada di projek lain, maka hanya perlu referensi ke project yang tepat, 
       bukan ke proyek itu sendiri -->
  
</Project>
